.container.col-md-12(ng-controller="tmContractDetailsCtrl as vm")
    h1 {{vm.contract.name}}
    .row
        .col-md-6
            h3(ng-show="vm.contract.contract") Contract
            h3 Contract Date {{vm.contract.date | date}}

        .col-md-6
            .panel.panel-primary
                .panel-heading Tags
                .panel-body
                        div(ng-repeat="tag in vm.contract.tags") {{tag}}